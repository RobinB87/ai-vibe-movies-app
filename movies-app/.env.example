# Environment variables for the Next.js application
# This file is meant to be copied to .env and filled with actual values.

# Database connection URL for Prisma
DATABASE_URL="postgresql://user:password@host:port/database?schema=public"

# Allowed origin for CORS. Can be a single URL (e.g., "https://my-frontend.com")
# or a comma-separated list of URLs (e.g., "http://localhost:3000,https://my-frontend.com")
# localhost will always be allowed by the CORS utility.
CORS_ALLOWED_ORIGIN="http://localhost:3000, http://localhost, https://my-frontend.com"

REDIS_PORT=6379
REDIS_HOST=127.0.0.1